{
  "name": "hoopr_v2",
  "version": "1.0.0",
  "description": "hoopr-revamped",
  "main": "server.js",
  "type": "module",
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "server:debug": "nodemon --inspect backend/server.js",
    "client": "npm start --prefix frontend_ts_v2",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcampuzano101/hoopr_v2.git"
  },
  "author": "David Campuzano",
  "license": "ISC",
  "dependencies": {
    "@sanity/client": "^2.1.4",
    "aws-sdk": "^2.783.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "docker-compose": "^0.23.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^8.119.0",
    "tinyurl": "^1.1.7"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  },
  "bugs": {
    "url": "https://github.com/dcampuzano101/hoopr_v2/issues"
  },
  "homepage": "https://github.com/dcampuzano101/hoopr_v2#readme"
}
